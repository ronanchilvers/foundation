{
    "name": "ronanchilvers/foundation",
    "description": "Foundation classes for use in php projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronan Chilvers",
            "email": "ronan@thelittledot.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ronanchilvers\\Foundation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronanchilvers\\Foundation\\Tests\\": "tests"
        }
    },
    "suggest": {
        "psr/container": "^1.0 If using Ronanchilvers\\Foundation\\Psr\\ContainerAwareTrait",
        "psr/log": "^1.0 If using Ronanchilvers\\Foundation\\Psr\\LoggerAwareTrait",
        "slim/slim": "^3.0 If using the Slim related classes",
        "symfony/console": "^3.4 If using console related classes",
        "pda/pheanstalk": "^3.2 If using the queue classes",
        "league/flysystem": "^1.0 If using the Filesystem classes"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "psr/container": "^1.0@dev",
        "psr/log": "^1.1@dev",
        "symfony/console": "^3.4",
        "slim/slim": "^3.0",
        "slim/twig-view": "dev-master",
        "psr/http-message": "^1.0@dev"
    }
}
